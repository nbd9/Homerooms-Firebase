{
  "rules": {
    "users": {
      "$uid": {
        ".read": "$uid === auth.uid",
        ".write": "$uid === auth.uid"
      }
    },
    "teachers": {
      ".read": "auth != null",
      ".write": false
    },
    "requests": {
      ".read": "auth != null",
      ".write": "auth != null"
    }
  }
}
